{
  "version": 2,
  "name": "elca-blockbusters",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/out"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://elca-blockbusters-api.onrender.com/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}

